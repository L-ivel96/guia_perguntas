<!DOCTYPE html>
  <head>
    <title>In√≠cio</title>
    <link rel="icon" type="image/x-icon" href="./assets/favicon.ico" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/css/estilo.css">
    <link rel="stylesheet" href="/css/bootstrap_5_1_0/css/bootstrap.min.css">
  </head>

  <body>
    <h1>Guia de Perguntas</h1>
    <hr />
    <h3>Lista de perguntas</h3>
    
    <p>NOME: <%= nome %></p>
    <p>LINGUAGEM: <%= lang %></p>
    <p>EMPRESA: <%= empresa %></p>
    <p>INSCRITOS: <%= inscritos %></p>

    <ul>
        <% produtos.forEach(function(produto) { %>
            <hr>
            <li>Produto: <%= produto.nome %> (R$<%= produto.preco %>)</li>
        <% }) %>
    </ul>
  </body>
  <footer>
    <% if(exibirMsg == true) { %>
        <h3>Mesnagem de ERRO</h3>
    <% } else { %>
        <h3>Nenhum erro detectado</h3>
    <% } %>
  </footer>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.3/dist/umd/popper.min.js" integrity="sha384-eMNCOe7tC1doHpGoWe/6oMVemdAVTMs2xqW4mwXrXsW0L84Iytr2wi5v2QjrP/xp" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.min.js" integrity="sha384-cn7l7gDp0eyniUwwAZgrzD06kc/tftFf19TOAs2zVinnD/C7E91j9yyk5//jjpt/" crossorigin="anonymous"></script>
  <script src="/css/bootstrap_5_1_0/js/bootstrap.min.js"></script>
